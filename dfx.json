    },
    "minter": {
      "candid": "packages/minter/minter.did",
      "package": "minter",
      "build": "./scripts/build-minter.sh",
      "wasm": "target/wasm32-unknown-unknown/release/minter.gz",
      "type": "custom"
    },
    "nft": {
      "candid": "dependencies/icrc7_launchpad/src/icrc7/icrc7.did",
      "package": "nft",
      "build": "./scripts/build-nft.sh",
